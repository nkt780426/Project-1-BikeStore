package controller.admin;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import view.admin.CustomerAdminDialog;

public class CustomerDialogController implements ActionListener  {
	private CustomerAdminDialog customerDialog;
	public CustomerDialogController(CustomerAdminDialog customerDialog) {
		this.customerDialog = customerDialog;
	}
	@Override
	public void actionPerformed(ActionEvent e) {
		customerDialog.dispose();
	}

}
